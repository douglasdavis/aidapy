Job: "myAIDAfit"
  CmeLabel: "13 TeV"
  POI: "mu_TTBAR"
  ReadFrom: HIST
  HistoPath: "/Users/ddavis/Software/aidapy"
  Lumi: 1.0
  LumiLabel: "36.1 fb^{-1}"
  MCstatThreshold: 0.01
  ImageFormat: "pdf"
  SystControlPlots: TRUE

Fit: "fit"
  FitType: SPLUSB
  FitRegion: CRSR
  FitBlind: FALSE
  NumCPU: 2

Region: "met_1pj"
  Type: SIGNAL
  HistoFile: "aida_histograms_met_1pj"
  VariableTitle: "#it{E}_{T}^{miss} [GeV]"
  ShortLabel: "#geq 1 jet"
  Label: "#geq 1 jet"
  TexLabel: "$N_{\mathrm{jets}} \geq 1$"
  DataType: ASIMOV

Region: "met_0j"
  Type: SIGNAL
  HistoFile: "aida_histograms_met_0j"
  VariableTitle: "#it{E}_{T}^{miss} [GeV]"
  ShortLabel: "= 0 jet"
  Label: "= 0 jet"
  TexLabel: "$N_{\mathrm{jets}} = 0$"
  DataType: ASIMOV

Sample: "Data"
  Title: "Data"
  Type: DATA
  HistoName: "Data"
  Regions: "met_1pj","met_0j"

Sample: "ttbar"
  Type: SIGNAL
  Title: "t#bar{t}"
  FillColor: 0
  LineColor: 1
  HistoName: "ttbar_FULL_main"
  NormFactor: "mu_TTBAR",1,-10,100
  Regions: "met_1pj","met_0j"

Sample: "Wt"
  Type: BACKGROUND
  Title: "Wt"
  FillColor: 62
  LineColor: 1
  HistoName: "Wt_FULL_main"
  Regions: "met_1pj","met_0j"

Sample: "Ztautau"
  Type: BACKGROUND
  Title: "Z#rightarrow#tau#tau"
  FillColor: 32
  LineColor: 1
  HistoName: "Ztautau_FULL_main"
  NormFactor: "mu_ZTAUTAU",1,-10,100
  Regions: "met_1pj","met_0j"

Sample: "WW"
  Type: BACKGROUND
  Title: "WW"
  FillColor: 42
  LineColor: 1
  HistoName: "WW_FULL_main"
  NormFactor: "mu_WW",1,-10,100
  Regions: "met_1pj","met_0j"

Sample: "Diboson"
  Type: BACKGROUND
  Title: "Diboson"
  FillColor: 22
  LineColor: 1
  HistoName: "Diboson_FULL_main"
  Regions: "met_1pj","met_0j"

Sample: "Fakes"
  Type: BACKGROUND
  Title: "Fakes"
  FillColor: 72
  LineColor: 1
  HistoName: "Fakes_FULL_main"
  Regions: "met_1pj","met_0j"

Sample: "Rare"
  Type: BACKGROUND
  Title: "RareSM"
  FillColor: 12
  LineColor: 1
  HistoName: "RareSM_FULL_main"
  Regions: "met_1pj","met_0j"

NormFactor: "mu_TTBAR"
  Title: "#mu_{t#bar{t}}"
  Nominal: 1
  Min: -10
  Max: 100
  Samples: ttbar

NormFactor: "mu_ZTAUTAU"
  Title: "#mu_{Z#rightarrow#tau#tau}"
  Nominal: 1
  Min: -10
  Max: 100
  Samples: Ztautau

NormFactor: "mu_WW"
  Title: "#mu_{WW}"
  Nominal: 1
  Min: -10
  Max: 100
  Samples: WW

Systematic: "ttbar_AR"
  Title: "ttbar Additional Radiation"
  Type: HISTO
  Samples: ttbar
  HistoNameUp:   "ttbar_FULL_sysARup"
  HistoNameDown: "ttbar_FULL_sysARdown"
  Regions: met_1pj,met_0j

Systematic: "ttbar_HS"
  Title: "ttbar Hard Scatter"
  Type: HISTO
  Samples: ttbar
  HistoNameUp: "ttbar_FULL_sysHS"
  %Symmetrisation: ONESIDED
  Regions: met_1pj,met_0j

Systematic: "ttbar_FH"
  Title: "ttbar Factorization/Hadronization"
  Type: HISTO
  Samples: ttbar
  HistoNameUp: "ttbar_FULL_sysFH"
  %Symmetrisation: ONESIDED
  Regions: met_1pj,met_0j

Systematic: "Wt_AR"
  Title: "Wt Additional Radiation"
  Type: HISTO
  Samples: Wt
  HistoNameUp:   "Wt_FULL_sysARup"
  HistoNameDown: "Wt_FULL_sysARdown"
  Regions: met_1pj,met_0j

Systematic: "Wt_DR_DS"
  Title: "Wt DR vs DS"
  Type: HISTO
  Samples: Wt
  HistoNameUp: "Wt_FULL_sysDS"
  %Symmetrisation: TWOSIDED
  Regions: met_1pj,met_0j

Systematic: "EG_RESOLUTION_ALL"
  Title: "Egamma Reso"
  Type: HISTO
  Samples: ttbar,Wt,WW,Ztautau,Diboson,Fakes,RareSM
  HistoNameSufUp: "_EG_RESOLUTION_ALL__1up"
  HistoNameSufDown: "_EG_RESOLUTION_ALL__1down"
  Regions: met_1pj,met_0j

Systematic: "EG_SCALE_ALL"
  Title: "Egamma Scale"
  Type: HISTO
  Samples: ttbar,Wt,WW,Ztautau,Diboson,Fakes,RareSM
  HistoNameSufUp: "_EG_SCALE_ALL__1up"
  HistoNameSufDown: "_EG_RESOLUTION_ALL__1down"
  Regions: met_1pj,met_0j
